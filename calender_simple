import calendar

def print_calendar():
    """
    Prints the calendar of a user-given month and year.
    """
    # Get user input for year and month
    year = int(input("Enter the year: "))
    month = int(input("Enter the month (1-12): "))
    
    # Print the calendar
    print(calendar.month(year, month))

# Example usage
print_calendar()
